{
  "enabled": true,
  "ruleName": "EasyTrade [manager | .NET] - GET /api/Packages/GetPackageById/{id}",
  "triggers": [
    {
      "source": {
        "dataSource": "request.method"
      },
      "type": "EQUALS",
      "value": "GET",
      "caseSensitive": false
    },
    {
      "source": {
        "dataSource": "request.path"
      },
      "type": "STARTS_WITH",
      "value": "/api/Packages/GetPackageById",
      "caseSensitive": false
    }
  ],
  "event": {
    "provider": {
      "sourceType": "constant.string",
      "source": "www.easytrade.com"
    },
    "type": {
      "sourceType": "constant.string",
      "source": "com.easytrade.manager.package"
    },
    "category": {
      "sourceType": "request.path"
    },
    "data": [
      {
        "name": "Support",
        "source": {
          "sourceType": "response.body",
          "path": "support"
        }
      },
      {
        "name": "Price",
        "source": {
          "sourceType": "response.body",
          "path": "price"
        }
      },
      {
        "name": "Name",
        "source": {
          "sourceType": "response.body",
          "path": "name"
        }
      },
      {
        "name": "Package ID",
        "source": {
          "sourceType": "response.body",
          "path": "id"
        }
      }
    ]
  }
}